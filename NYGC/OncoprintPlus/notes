cat ../../../../../metadata/tumor_normal_pairs_bel.txt | while read tumor normal remainder; do cut -f1-4 ../../../jabba/gene-cn/${tumor}--${normal}.simple.cnv.bed | bedtools intersect -wo -a /gpfs/commons/resources/GRCh38_full_analysis_set_plus_decoy_hla/internal/ensembl_genes_unique_sorted.final.v93.short.chr.sorted.bed -b stdin | sed "s/$/\t${tumor}\t${normal}/" > ${tumor}--${normal}.jabba.bed ; done